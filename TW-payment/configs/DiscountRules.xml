<?xml version="1.0" encoding="UTF-8"?>
<rules>
	<rule>
		<name>第二件半价</name>
		<jarFileName>Ruls_Buy3Free1And95off.jar</jarFileName>
		<jarFilePath>rules</jarFilePath>
		<className>com.tw.bill.rule.Ruls_Buy3Free1</className>
