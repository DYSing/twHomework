		<jarFilePath>rules</jarFilePath>
		<className>com.tw.bill.rule.Ruls_AllGoods95</className>
		<level>2</level>
		<goods>ITEM000001#ITEM000002#ITEM000003#ITEM000004#ITEM000005</goods>
	</rule>
